{
  "main": "webpack.config.js",
  "name": "react-thunk-calculator",
  "version": "1.0.0",
  "description": "Simple React Calculator With Thunk Middleware",
  "scripts": {
    "webpack-config-dev": "nodemon --watch \"./webpack/*\" --exec npm run start-dev",
    "webpack-config-prod": "nodemon --watch \"./webpack/*\" --exec npm run start-prod",
    "start-dev": "webpack serve --config webpack/dev.config.js",
    "start-prod": "webpack serve --config webpack/prod.config.js",
    "build-dev": "webpack --config webpack/dev.config.js",
    "build-prod": "webpack --config webpack/prod.config.js --stats-children",
    "clean": "rd /s /q build",
    "lint": "eslint src --ext .js --ext .ts",
    "lint-fix": "eslint src --ext .js --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
